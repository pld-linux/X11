+static
+int compare(const void *arg1, const void *arg2)
+{
+    const FontList *f1 = arg1;
+    const FontList *f2 = arg2;
+    const char *p1 = f1->name;
+    const char *p2 = f2->name;
+
+    while (*p1 && *p2 && *p1 == *p2)
+            p1++, p2++;
+    return(*p1 - *p2);
+}
