+    case 0x15:			/* left_tee             */
+    case 0x16:			/* right_tee            */
+    case 0x17:			/* bottom_tee           */
+    case 0x18:			/* top_tee              */
+    case 0x19:			/* vertical_line        */
+	result = True;
+	TRACE(("xtermIsLineDrawing %d\n", wc));
+	break;
+    default:
