+    Bool result;
+    switch (wc) {
+    case 0x0B:			/* lower_right_corner   */
+    case 0x0C:			/* upper_right_corner   */
+    case 0x0D:			/* upper_left_corner    */
+    case 0x0E:			/* lower_left_corner    */
+    case 0x0F:			/* cross                */
