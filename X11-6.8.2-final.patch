+static
+void copy_number(char **pp1, char**pp2, int n1, int n2)
+{
+    char *p1 = *pp1;
+    char *p2 = *pp2;
+    int   w;
+
+    sprintf(p1, "%d", n1);
+    sprintf(p2, "%d", n2);
+    w = MAX(strlen(p1), strlen(p2));
+    sprintf(p1, "%*d", w, n1);
+    sprintf(p2, "%*d", w, n2);
+    p1 += strlen(p1);
+    p2 += strlen(p2);
+    *pp1 = p1;
+    *pp2 = p2;
